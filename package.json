{
  "name": "nextjs-blog-markdown",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Asad Mujumder",
    "email": "asaduzzaman.anik.me@gmail.com",
    "url": "https://asad13.github.io"
  },
  "description": "A Blog using Next.js and Tailwind CSS. Blog Articles are stored as Markdown Files.",
  "keywords": ["nextjs","tailwindcss", "react", "blog", "markdown", "gray-matter", "remark", "yaml", "@svgr/webpack"],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "next build && mkdir out && nul > ./out/.nojekyll && git add out/ && git commit -m \"Deploy\" && git subtree push --prefix out origin gh-pages"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  }
}
